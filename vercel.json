{
  "version": 2,
  "functions": {
    "scripts/contentful-vercel-webhook-filter.js": {
      "maxDuration": 30
    }
  },
  "headers": [
    {
      "source": "/api/webhook/contentful",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        },
        {
          "key": "Access-Control-Allow-Methods",
          "value": "POST, OPTIONS"
        },
        {
          "key": "Access-Control-Allow-Headers",
          "value": "Content-Type, x-contentful-signature"
        }
      ]
    }
  ]
}
